<%= turbo_frame_tag dom_id(task) do %>
  <div class="card card-body">
    <div>
      <%= check_box_tag('box', value = '1', checked = task.completed) %>
      <%= task.title %> - <%= task.details %>
      <%= link_to "Edit", edit_list_task_path(task.list_id, task.id) %>
    </div>
  </div>
<% end %>
